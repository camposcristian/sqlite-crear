
head 
 <!-- Stylesheets -->
 <link rel="stylesheet" href="css/layout.css">
<body class="cuerpo">
- var user=localStorage.getItem('user')
<div class="barra"><p class="bienvenido"> Hola #{user}! &nbsp;&nbsp;<div class="enlace2"><a href=cierresesion>Cerrar Sesion</a></div></div>
<div class="imag"><img class="logo" src="../images/logocrear2.png">
<p class="info">
br
</div>
br
br
br 
br
br
<div class="informe2"> <p class="informetext">#{nombre2}</p></div>
<table class="tabla" border="solid">
tr
  <td class="id"> <h4> ID
  td <h4> Nombre 
  td <h4> Apellido
  td <h4> Cedula
  <td class="fecha">  <h4>fechanac
  <td class="opciones"> <h4> Empresa
  td <h4>Departamento
  <td class="fecha">  <h4>Modificado
  <td class="id"> <h4>Acceso
  <td class="id"><h4>Huella
  <td class="opciones"><h4>Opciones
tr
- for (var x = 0; x < max; x++)
   <td class="id"> #{vector[x].id} 
   <td> #{vector[x].nombre} 
   <td> #{vector[x].apellido}
   <td> #{vector[x].cedula}
   <td class="fecha">  #{vector[x].fechanac}
   <td class="opciones"> #{vector[x].empresa}
   <td> #{vector[x].dpto}   
   <td class="fecha">  #{vector[x].idusuario}
   
   if vector[x].acceso == "true"
        <td class="id"> <input type="checkbox" name="acceso" checked="true" disabled /> 
   else
        <td class="id"> <input type="checkbox" name="acceso"  disabled /> 
   if vector[x].huella===null||vector[x].huella==="Sin Enrolar"
        <td class="id"> 
   else
        <td class="id"> <img src="../images/huellacla.png" width="15" height="20" name="acceso"/>   
   <td class="id">
   <a href=empleados/editar/#{vector[x].id}><input type=image src="../images/editarcla.png" width="20" height="20" name="editar"></a>&nbsp;
   <a href=empleados/confirmar/#{vector[x].id}><input type=image src="../images/borrarcla.png" width="20" height="20" name="borrar"></a>&nbsp;
   <a href="javascript:document.location.reload();"><img src="../images/actualizarcla.png" width="20" height="20" text="actualizar" name="actualizar" />
   if (x%2)==0
    <tr class="par">   
   else 
    tr
</table>
<p class="version"> Version: #{version}

<!-- JS  -->
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.js"></script>
<script>window.jQuery || document.write("<script src='js/jquery-1.5.1.min.js'>\x3C/script>")</script>
<script src="js/app.js"></script>
